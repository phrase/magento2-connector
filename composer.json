{
  "name": "memsource/magento2-connector",
  "description": "Memsource Cloud connector for Magento 2",
  "type": "magento2-module",
  "license": ["MIT"],
  "version": "2.0.0",
  "require": {
    "php": "~5.6.5|~7.0|~8.0",
    "magento/framework": "~100.1|~101.0|~102.0|~103.0"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Memsource\\Connector\\": ""
    }
  },
  "config": {
    "allow-plugins": {
      "laminas/laminas-dependency-plugin": true,
      "magento/composer-dependency-version-audit-plugin": true,
      "magento/magento-composer-installer": true
    }
  }
}
